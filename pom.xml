            <dependency>
                <groupId>io.github.lvyahui8</groupId>
                <artifactId>feego-common-web-starter</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>io.github.lvyahui8</groupId>
                <artifactId>feego-common-example</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>io.github.lvyahui8</groupId>
